<% include partials/header %>

    <div class="container text-center">
      <div class="jumbotron">
        <h1>See them tweets!</h1>
        <form action="/tweets" method="GET">
          <button class='btn btn-success btn-lg'>see em!</button>
        </form>
    </div>

    <div class="container text-center" style="border: 2px solid black">
      <h3>username: <%= userName %></h3>
      <form action="/retweet" method="POST">
        <button>Retweet Selected</button>
      </form>
      <% for(let i = 0; i < tweets.length; i++) { %>
        <ul>
          <li><%= tweets[i].tweetbody %><label>Retweet:<input type='checkbox'></label></li>
        </ul>
      <% } %>
    </div>

<% include partials/footer %>
